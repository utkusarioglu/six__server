{
  "name": "six__server",
  "version": "1.0.0",
  "main": "packages/main/lib/index.js",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "dependencies": {
    "@types/node": "^14.14.16",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:dev": "run-p start:watcher start:watch",
    "start:watcher": "nodemon",
    "start:watch": "$(yarn bin)/lerna run --parallel start:watch",
    "clean": "lerna run --parallel clean",
    "test:inside-container": "$(yarn bin)/jest",
    "test:ci": "$(yarn bin)/jest test --coverage --no-watchman a",
    "test": "docker exec -it six__server bash -c '$(yarn bin)/jest'",
    "bash": "docker exec -it six__server bash"
  }
}
