{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "composite": true,
    "removeComments": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "six__server__store": ["./packages/store/src"],
      "six__server__auth": ["./packages/auth/src"],
      "six__server__main": ["./packages/main/src"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
